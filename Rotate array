class Solution {
    public void rotate(int[] nums, int k) {
        k%=nums.length;
      reverse(0,nums.length-1,nums);
      reverse(0,k-1,nums);
      reverse(k,nums.length-1,nums);
    
        
    }
    public void reverse(int i, int j, int[] arr){
        while(i<=j){
            int temp=arr[i];
            arr[i]=arr[j];
            arr[j]=temp;
            j--;
            i++;
        }
    }
}
//Time complexity:O(n)

//Space complexity: O(1)
